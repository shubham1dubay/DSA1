#include<stdio.h>
#include<conio.h>
void main()
{
int a[10];
int i,n,item;
clrscr();
printf("Enter the size of array\n");
scanf("%d",&n);
printf("Enter the Element of array \n");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("entered array is:\n");
for(i=0;i<n;i++)
{
printf("%d  ",a[i]);
}
printf("\n Enter the item ");
scanf("%d",&item);
for(i=0;i<n;i++)
{
if(item==a[i])
{
printf("item found at %d",i+1);
break;
}
if(i==n)
{
printf("Item not found");
}
}
getch();
}
